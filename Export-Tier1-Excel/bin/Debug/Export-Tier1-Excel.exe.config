<?xml version="1.0"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
    <appSettings>
      <add key="SiteURL" value="https://www.site1.com/subsite" />  
      <add key="UserName" value="add_user_name" />  <!-- This is the name of the list copying list items from-->
      <add key="Password" value = "add_password" />
      <add key="ListName" value="the list you want to pull data from" />
      <add key="ViewName" value="the view to the above list" />
      <add key="ExcelName" value="the name of excel spreadsheet to create" />
      <add key="DestinationList" value="this is where excel spreadsheet will be saved to" />
      <add key="ExportTempLocation" value="location on local computer, needed by excel.  Like: @ C:\temp\Tier1_ICG_Project_Status"/>
      <add key="ExportTempDirectory" value="" />
    </appSettings>
</configuration>
